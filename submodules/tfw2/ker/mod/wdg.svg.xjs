{View SVG
  view.attribs: {
    width: {integer 200}
    height: {integer 200}
    viewbox: {string "-100 -100 200 200"}
    content: {any {} behind: onContentChanged}
  }

  version: "1.1"
  width: {Bind width}
  height: {Bind height}
  viewbox: {Bind viewbox}
  preserveaspectratio: "xMidYMid"
  stroke: #000
  fill: none
  stroke-width: 1

  [
    {PATH stroke:#f80 stroke-width:5 fill:none d:"M0,0 L100,100 v-200 h-150 v100"}
  ]
}