{View IMG class:tfw-view-image
  view.attribs: {
    src: {string "" behind:onSrcChanged}
    loaded: {boolean false}
    failed: {boolean false}
    width: {unit auto behind:onWidthChanged}
    height: {unit auto behind:onHeightChanged}
    max-width: {unit auto behind:onMaxWidthChanged}
    max-height: {unit auto behind:onMaxHeightChanged}
  }

  src: {Bind src}
  
  event.load: onLoad
  event.error: onError
}